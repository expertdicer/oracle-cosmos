            "block_height",
            "distributed_interest"
          ],
          "properties": {
            "block_height": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "distributed_interest": {
              "$ref": "#/definitions/Uint256"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Uint256": {
      "type": "string"
    }
  }
}
